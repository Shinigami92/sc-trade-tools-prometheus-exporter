    "dev": "node --env-file .env ./dist/index.js",
    "preflight": "pnpm install && run-s build dev"
  },
  "keywords": [
    "esm",
    "fetch-api",
    "fetch",
    "star citizen",
    "typescript"
  ],
  "homepage": "https://github.com/Shinigami92/sc-trade-tools-prometheus-exporter",
  "bugs": {
    "url": "https://github.com/Shinigami92/sc-trade-tools-prometheus-exporter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shinigami92/sc-trade-tools-prometheus-exporter.git"
  },
  "author": {
    "name": "Christopher Quadflieg",
    "email": "chrissi92@hotmail.de",
    "url": "https://github.com/Shinigami92"
  },
  "type": "module",
  "exports": {
